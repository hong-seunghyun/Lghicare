{
  "name": "lg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider next dev",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider next build",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider next start",
    "lint": "eslint",
    "export": "next export",
    "build:ssr": "npm run build && powershell -Command \"Remove-Item -Recurse -Force functions/.next; cp -r .next functions/\"",
    "deploy:ssr": "npm run build:ssr && firebase deploy --only \"functions,hosting\""
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "firebase": "^12.2.1",
    "firebase-functions-test": "^3.4.1",
    "googleapis": "^159.0.0",
    "next": "^15.5.3",
    "next-auth": "^4.24.11",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.34",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.4.0",
    "firebase-tools": "^14.17.0",
    "typescript": "^5"
  }
}
